{
  "rewrites": [
    { "source": "/studio", "destination": "/studio.html" },
    { "source": "/studio/", "destination": "/studio.html" },
    { "source": "/login", "destination": "/login.html" },
    { "source": "/merch", "destination": "/themeqr-merch.html" },
    { "source": "/landing", "destination": "/landing.html" }
  ],
    "redirects": [
    { "source": "/studio/studio.html", "destination": "/studio", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}